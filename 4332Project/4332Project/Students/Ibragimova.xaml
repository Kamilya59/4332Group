<Window x:Class="_4332Project.Students.Ibragimova"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4332Project.Students"
        mc:Ignorable="d"
        Title="Ibragimova" Height="450" Width="800">
    <StackPanel Background="Azure" Margin="0,0,-132,-16">
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50">Ибрагимова К.Р. 4332</TextBlock>
        <Button x:Name="ExportButton" Content="Импорт данных" Margin="30 10 0  0" Width="150"  Click="Button_Click"></Button>
        <Button x:Name="ImportButton" Content="Экспорт в Excel" Margin="0 10 30 0" Width="150"  Click="Button_Click_1"></Button>
        <Button x:Name="JsonImportButton" Content="Импорт JSON данных" Margin="0 10 30 0" Width="150" Click="JsonImportButton_Click"/>
        <Button x:Name="JsonExportButton" Content="Экспорт в Word" Margin="30 10 0  0" Width="150" Click="JsonExportButton_Click"/>
    </StackPanel>
</Window>
